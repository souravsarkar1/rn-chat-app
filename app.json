{
  "expo": {
    "name": "Vibe Chat",
    "slug": "vibe-chat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/vibe_chat.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/vibe_chat.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.souravsarkar1.vibechat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/vibe_chat.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/vibe_chat.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        },
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "df1c9e17-0b86-466b-ac39-7863fbba4db3"
      }
    }
  }
}